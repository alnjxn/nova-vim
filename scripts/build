#!/bin/bash
set -euo pipefail

# ./node_modules/eslint/bin/eslint.js .
warning='" WARNING: Do not edit this file directly - it is built from the src directory'
echo $warning > colors/oceanic-next.vim
echo $warning > autoload/airline/themes/oceanic-next.vim
node src/index.js >> colors/oceanic-next.vim
node src/airline.js >> autoload/airline/themes/oceanic-next.vim
